{
    "name": "ElxaOS Dark",
    "id": "elxaosdark",
    "colors": {
        "--taskbar-bg": "#28282b",
        "--taskbar-border": "#2a2a2e",
        "--start-button-text": "#a0a0a0",
        "--start-button": "#353538",
        "--start-menu-bg": "#28282b",
        "--start-menu-text": "#727074",
        "--start-menu-hover": "#383738",
        "--start-menu-hover-text": "#ffffff",
        "--accent-text": "#8D7D6F",
        "--header-text": "#AC9784",
        "--purple-darker": "#1a1a1d",
        "--purple-dark": "#2a2a2e",
        "--purple-medium": "#3a3a40",
        "--purple-main": "#4a4a52",
        "--purple-light": "#5a5a64",
        "--bg-lightest": "#524F4D",
        "--bg-light": "#2E2D2D",
        "--bg-mid": "#222121",
        "--border-light": "#404045",
        "--border-dark": "#2a2a2e",
        "--accent-blue": "#d4a373",
        "--accent-blue-light": "#e3b587",
        "--accent-pink": "#b08968",
        "--nav-button-disabled": "#1f1f22",
        "--text-black": "#e8e8e8",
        "--text-muted": "#a0a0a0",
        "--text-disabled": "#666666",
        "--text-light": "#242323",
        "--warning-mid": "#cc6666",
        "--warning-light": "#994d4d",
        "--warning-lighter": "#803333",
        "--warning-text": "#ff9999",
        "--ui-disabled-bg": "#1f1f22",
        "--ui-disabled-border": "#2a2a2e",
        "--bg-explorer-window": "#3E3E3F"
    },
    "gradients": {
        "--bg-gradient": "linear-gradient(135deg, var(--purple-main), var(--accent-blue), var(--accent-pink))",
        "--titlebar-gradient": "linear-gradient(to right, var(--purple-main), var(--accent-blue))",
        "--button-gradient": "linear-gradient(to bottom, var(--bg-light), var(--bg-mid))",
        "--button-hover-gradient": "linear-gradient(to bottom, var(--bg-mid), var(--border-light))"
    },
    "transparentColors": {
        "--bg-dark-transparent": "rgba(42, 42, 46, 0.2)",
        "--bg-mid-transparent": "rgba(40, 40, 43, 0.3)",
        "--bg-mid-selected": "rgba(40, 40, 43, 0.5)",
        "--text-light-transparent": "rgba(255, 255, 255, 0.4)",
        "--purple-medium-alpha": "rgba(58, 58, 64, 0.2)",
        "--window-shadow": "rgba(0, 0, 0, 0.4)",
        "--shadow-medium": "rgba(0, 0, 0, 0.25)",
        "--shadow-light": "rgba(0, 0, 0, 0.15)"
    }
}
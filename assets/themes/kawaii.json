{
    "name": "Kawaii",
    "id": "kawaii",
    "colors": {
        "--taskbar-bg": "#ffc7d9",
        "--taskbar-border": "#ffdce7",
        "--start-button-text": "#7a2049",
        "--start-button": "#ffdce7",
        "--start-menu-bg": "#ffdce7",
        "--start-menu-text": "#5c1334",
        "--start-menu-hover": "#ffb3c6",
        "--start-menu-hover-text": "#ffffff",
        "--accent-text": "#7a2049",
        "--header-text": "#7a2049",
        "--purple-darker": "#5c1334",
        "--purple-dark": "#7a2049",
        "--purple-medium": "#ff6f91",
        "--purple-main": "#ff99b9",
        "--purple-light": "#ffc7d9",
        "--bg-lightest": "#ffeef3",
        "--bg-light": "#ffdce7",
        "--bg-mid": "#ffc7d9",
        "--border-light": "#ffb3c6",
        "--border-dark": "#cc718e",
        "--accent-blue": "#8fbfff",
        "--accent-blue-light": "#add8ff",
        "--accent-pink": "#fff0a6",
        "--nav-button-disabled": "#f8f8f8",
        "--text-black": "#000000",
        "--text-muted": "#777777",
        "--text-disabled": "#aaaaaa",
        "--text-light": "#ffffff",
        "--warning-mid": "#ff9999",
        "--warning-light": "#ffcccc",
        "--warning-lighter": "#ffeded",
        "--warning-text": "#cc0000",
        "--ui-disabled-bg": "#f8f8f8",
        "--ui-disabled-border": "#dddddd",
        "--bg-explorer-window": "#ffffff"
    },
    "gradients": {
        "--bg-gradient": "linear-gradient(135deg, var(--purple-main), var(--accent-blue), var(--accent-pink))",
        "--titlebar-gradient": "linear-gradient(to right, var(--purple-main), var(--accent-blue))",
        "--button-gradient": "linear-gradient(to bottom, var(--bg-light), var(--bg-mid))",
        "--button-hover-gradient": "linear-gradient(to bottom, var(--bg-mid), var(--border-light))"
    },
    "transparentColors": {
        "--bg-dark-transparent": "rgba(204, 113, 142, 0.2)",
        "--bg-mid-transparent": "rgba(255, 199, 217, 0.3)",
        "--bg-mid-selected": "rgba(255, 199, 217, 0.5)",
        "--text-light-transparent": "rgba(255, 255, 255, 0.4)",
        "--purple-medium-alpha": "rgba(122, 32, 73, 0.2)",
        "--window-shadow": "rgba(0, 0, 0, 0.3)",
        "--shadow-medium": "rgba(0, 0, 0, 0.15)",
        "--shadow-light": "rgba(0, 0, 0, 0.1)"
    }

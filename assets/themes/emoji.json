{
    "name": "Emoji",
    "id": "emoji",
    "colors": {
        "--taskbar-bg": "#ff6b6b",
        "--taskbar-border": "#ff9999",
        "--start-button-text": "#7a0000",
        "--start-button": "#ff9999",
        "--start-menu-bg": "#ff9999",
        "--start-menu-text": "#660000",
        "--start-menu-hover": "#ff4d4d",
        "--start-menu-hover-text": "#ffffff",
        "--accent-text": "#7a0000",
        "--header-text": "#7a0000",
        "--purple-darker": "#660000",
        "--purple-dark": "#7a0000",
        "--purple-medium": "#ff3333",
        "--purple-main": "#ff6666",
        "--purple-light": "#ff9999",
        "--bg-lightest": "#ffe5e5",
        "--bg-light": "#ffd6d6",
        "--bg-mid": "#ff9999",
        "--border-light": "#ff8080",
        "--border-dark": "#cc4c4c",
        "--accent-blue": "#67c9dc",
        "--accent-blue-light": "#89d7e6",
        "--accent-pink": "#ffcc33",
        "--nav-button-disabled": "#f8f8f8",
        "--text-black": "#000000",
        "--text-muted": "#777777",
        "--text-disabled": "#aaaaaa",
        "--text-light": "#ffffff",
        "--warning-mid": "#ff704d",
        "--warning-light": "#ffb3a7",
        "--warning-lighter": "#ffd6c9",
        "--warning-text": "#cc2900",
        "--ui-disabled-bg": "#f8f8f8",
        "--ui-disabled-border": "#dddddd",
        "--bg-explorer-window": "#ffffff"
    },
    "gradients": {
        "--bg-gradient": "linear-gradient(135deg, var(--purple-main), var(--accent-blue), var(--accent-pink))",
        "--titlebar-gradient": "linear-gradient(to right, var(--purple-main), var(--accent-blue))",
        "--button-gradient": "linear-gradient(to bottom, var(--bg-light), var(--bg-mid))",
        "--button-hover-gradient": "linear-gradient(to bottom, var(--bg-mid), var(--border-light))"
    },
    "transparentColors": {
        "--bg-dark-transparent": "rgba(204, 76, 76, 0.2)",
        "--bg-mid-transparent": "rgba(255, 153, 153, 0.3)",
        "--bg-mid-selected": "rgba(255, 153, 153, 0.5)",
        "--text-light-transparent": "rgba(255, 255, 255, 0.4)",
        "--purple-medium-alpha": "rgba(122, 0, 0, 0.2)",
        "--window-shadow": "rgba(0, 0, 0, 0.3)",
        "--shadow-medium": "rgba(0, 0, 0, 0.15)",
        "--shadow-light": "rgba(0, 0, 0, 0.1)"
    }
